    description: shows twitch link

  links:
    description: shows server links

  decp:
    description: shows the Decp information

  youtube:
    description: shows youtube link
    aliases: [yt]

  setgui:
    description: setup GUI

  ctc:
    description: Clear the chat
    aliases: [clearchat, ctchat, clchat]

  vanish:
    description: Go invisable
    aliases: [v, uv, unvanish]

  craft:
    description: portable workbench
    alisases: [wb, workbench, craftbench]

  trash:
    description: Throww away unwanted items
    aliases: [dispose, trash]

  fly:
    description: Fly mode

  speed:
    description: Change walk and fly speed

  gamemode:
    description: change gamemode
    aliases: [gm, gmode]

  feed:
    description: Fill your hunger
    permission: decp.feed

  heal:
    description:  Heal your hearts
    permission: decp.heal

  sun:
    description: sets the weather sunny
    aliases: [clear, sunny]

  rain:
    description: sets weather to storm
    aliases: [storm, thunder]

  day:
    description: set day time
  
  night:
    description: set night time

  tpa:
    description: Send a tpa request
    permission: decp.tpa.tpa

  tp:
    description: teleport to a player

  tpaccept:
    description: Accept a tpa request
    permission: decp.tpa.accept

  tpdeny:
    description: Decline a tpa request
    permission:  decp.tpa.deny

  tphere:
    description: teleport a player to you
    permission:  decp.tpa.tphere

  spawn:
    description: teleport to server spawn

  setspawn:
    description:  set server spawn point

  home:
    description: go to your home location

  sethome:
    description: Set your home location

  delhome:
    description: Delete a home

  listhome:
    description: list your homes

  serverinfo:
    description: Shows info about the server.
    aliases: [stats, info, statistics, serverstats, serverstatistics]

  repair:
    description: repair your tools and armor

  broadcast:
    description: Broadcast a message to the whole server
    aliases: [bcast]
   
  superpick:
    description: Toggle Superpick
    aliases: [sp, spick]

